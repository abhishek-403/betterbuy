
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Product {
  id  Int @id @default(autoincrement())
  name String
  price Int
  currency String
  image String
  provider String
  owner    User     @relation(fields: [ownerId], references: [id])
  ownerId  Int
}


model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  products Product[]
}